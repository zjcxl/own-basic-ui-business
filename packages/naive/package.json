{
  "name": "@own-basic/ui-business-naive",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.15.6",
  "author": "chenxueli",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zjcxl/own-basic-ui-business.git",
    "directory": "packages/naive"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 3333",
    "lint": "eslint naive",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.13.3",
    "@own-basic-component/buried": "^0.8.30",
    "@own-basic-component/config": "^0.8.30",
    "@own-basic-component/util": "^0.8.30",
    "@types/node": "^20.12.7",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/test-utils": "^2.4.5",
    "@vueuse/core": "^10.9.0",
    "date-fns": "^3.6.0",
    "eslint": "^9.0.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "naive-ui": "^2.38.1",
    "pnpm": "^8.15.6",
    "taze": "^0.13.3",
    "typescript": "^5.4.5",
    "unplugin-vue-macros": "^2.9.1",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.8.1",
    "vitest": "^1.5.0",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.12"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
