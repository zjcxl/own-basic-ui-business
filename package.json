{
  "name": "@own-team/base-ui-business",
  "private": true,
  "packageManager": "pnpm@8.15.6",
  "version": "0.0.4",
  "scripts": {
    "build": "pnpm -r run build",
    "build:stub": "pnpm -r run build:stub",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "bumpp": "bumpp patch -r --commit --push --tag -y",
    "publish:release": "pnpm -r publish --no-git-checks --access public",
    "publish:pre": "nr build",
    "up": "taze major -rfwi"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.13.3",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "bumpp": "^9.4.0",
    "eslint": "^9.0.0",
    "esno": "^4.7.0",
    "pnpm": "^8.15.6",
    "rimraf": "^5.0.5",
    "taze": "^0.13.3",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.8",
    "vitest": "^1.5.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
