{
  "name": "@own-team/base-ui-business",
  "private": true,
  "packageManager": "pnpm@9.14.2",
  "version": "0.0.318",
  "scripts": {
    "build": "pnpm -r run build",
    "build:stub": "pnpm -r run build:stub",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "bumpp": "bumpp patch -r --commit --push --tag -y",
    "publish:release": "pnpm -r publish --no-git-checks --access public",
    "publish:pre": "nr build",
    "up": "taze major -rfwi"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.10.0",
    "@antfu/ni": "^0.23.1",
    "@antfu/utils": "^0.7.10",
    "@volar/language-server": "^2.4.10",
    "@vue/language-server": "^2.1.10",
    "bumpp": "^9.8.1",
    "eslint": "^9.15.0",
    "esno": "^4.8.0",
    "pnpm": "^9.14.2",
    "rimraf": "^5.0.10",
    "taze": "^0.18.0",
    "typescript": "^5.7.2",
    "unbuild": "^2.0.0",
    "vite": "^6.0.0",
    "vitest": "^2.1.6"
  },
  "engines": {
    "node": ">=20.10.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
