{
  "name": "@own-team/base-ui-business",
  "private": true,
  "packageManager": "pnpm@9.1.0",
  "version": "0.0.97",
  "scripts": {
    "build": "pnpm -r run build",
    "build:stub": "pnpm -r run build:stub",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "bumpp": "bumpp patch -r --commit --push --tag -y",
    "publish:release": "pnpm -r publish --no-git-checks --access public",
    "publish:pre": "nr build",
    "up": "taze major -rfwi"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.17.0",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.8",
    "bumpp": "^9.4.1",
    "eslint": "^9.2.0",
    "esno": "^4.7.0",
    "pnpm": "^9.1.0",
    "rimraf": "^5.0.6",
    "taze": "^0.13.8",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=20.10.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
